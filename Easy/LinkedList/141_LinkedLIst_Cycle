/**
 * Definition for singly-linked list.
 * class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution 
{
    public boolean hasCycle(ListNode head) 
    {
        ListNode trav = head;
        HashSet<ListNode> dup = new HashSet();
        while (trav!= null)
        {
            if (dup.add(trav)) 
            {
                trav = trav.next;

            }
            else
            {
                return true;
                //break;
            }
        }
        return false;
        
    }
}